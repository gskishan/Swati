  "format_data": null,
  "html": "<h3 style=\"text-align:center; font-weight:bold;\">Bonus Sheet</h3>\n\n<table class=\"table table-bordered table-sm\" style=\"font-size:13px; text-align:center; border: 1px solid black; width: 100%;\">\n    <thead style=\" color: #fff;\">\n        <tr>\n            <th rowspan=\"2\" style=\"color:black; font-size:15px; border: 1px solid black;\"><strong>Employee No</strong></th>\n            <th rowspan=\"2\" style=\"color:black; font-size:15px; border: 1px solid black;\"><strong>Name</strong></th>\n            <th rowspan=\"2\" style=\"color:black; font-size:15px; border: 1px solid black;\"><strong>UAN</strong></th>\n            <th rowspan=\"2\" style=\"color:black; font-size:15px; border: 1px solid black;\"><strong>DOJ</strong></th>\n            <th colspan=\"3\" style=\"color:black; font-size:15px; border: 1px solid black; text-align:center;\"><strong>Master Salary</strong></th>\n\n            {# Step 1: Collect unique month_years #}\n            {% set months = [] %}\n            {% for row in doc.employee_salary_details %}\n                {% if row.month_year and row.month_year not in months %}\n                    {% set _ = months.append(row.month_year) %}\n                {% endif %}\n            {% endfor %}\n\n            {# Step 2: Add month-year columns #}\n            {% for month in months %}\n                <th colspan=\"2\" style=\"color:black; font-size:15px; border: 1px solid black; text-align:center;\"><strong>{{ month }}</strong></th>\n            {% endfor %}\n\n            <th colspan=\"5\" style=\"color:black; font-size:15px; border: 1px solid black; text-align:center;\"><strong>Fixed Days/Amount</strong></th>\n        </tr>\n        <tr>\n            <th style=\"color:black; font-size:15px; border: 1px solid black;\"><strong>Full Basic</strong></th>\n            <th style=\"color:black; font-size:15px; border: 1px solid black;\"><strong>Full VDA</strong></th>\n            <th style=\"color:black; font-size:15px; border: 1px solid black;\"><strong>Total</strong></th>\n\n            {% for month in months %}\n                <th style=\"color:black; font-size:15px; border: 1px solid black;\"><strong>No of Days</strong></th>\n                <th style=\"color:black; font-size:15px; border: 1px solid black;\"><strong>Basic + DA</strong></th>\n            {% endfor %}\n\n            <th style=\"color:black; font-size:15px; border: 1px solid black;\"><strong>Total Days</strong></th>\n            <th style=\"color:black; font-size:15px; border: 1px solid black;\"><strong>Total Gross</strong></th>\n            <th style=\"color:black; font-size:15px; border: 1px solid black;\"><strong>Bonus(8.33%)</strong></th>\n            <th style=\"color:black; font-size:15px; border: 1px solid black;\"><strong>Bonus Amount</strong></th>\n        </tr>\n    </thead>\n    <tbody>\n        <tr>\n            <td style=\"color:black; font-size:15px; border: 1px solid black;\">{{ doc.employee_no }}</td>\n            <td style=\"color:black; font-size:15px; border: 1px solid black;\">{{ doc.name1 }}</td>\n            <td style=\"color:black; font-size:15px; border: 1px solid black;\">{{ doc.uan }}</td>\n            <td style=\"color:black; font-size:15px; border: 1px solid black;\"> {{ frappe.utils.formatdate(doc.date_of_joining, \"dd/mm/yyyy\") if doc.date_of_joining else \"\" }}</td>\n            <td style=\"color:black; font-size:15px; border: 1px solid black;\">{{ doc.full_basic }}</td>\n            <td style=\"color:black; font-size:15px; border: 1px solid black;\">{{ doc.full_vda }}</td>\n            <td style=\"color:black; font-size:15px; border: 1px solid black;\">{{ doc.total }}</td>\n\n            {# Fetch no_of_days and basic_da based on month #}\n           {% for month in months %}\n    {% set ns = namespace(data_found=0) %}\n    {% for row in doc.employee_salary_details %}\n        {% if row.month_year == month and ns.data_found == 0 %}\n            <td style=\"color:black; font-size:15px; border: 1px solid black;\">{{ row.no_of_days }}</td>\n            <td style=\"color:black; font-size:15px; border: 1px solid black;\">{{ row.sum_of_basic_and_da }}</td>\n            {% set ns.data_found = 1 %}\n        {% endif %}\n    {% endfor %}\n    {% if ns.data_found == 0 %}\n        <td style=\"color:black; font-size:15px; border: 1px solid black;\">-</td>\n        <td style=\"color:black; font-size:15px; border: 1px solid black;\">-</td>\n    {% endif %}\n{% endfor %}\n\n            <td style=\"color:black; font-size:15px; border: 1px solid black;\">{{ doc.total_days }}</td>\n            <td style=\"color:black; font-size:15px; border: 1px solid black;\">{{ doc.total_gross }}</td>\n            <td style=\"color:black; font-size:15px; border: 1px solid black;\">{{ doc.bonus_833 }}</td>\n            <td style=\"color:black; font-size:15px; border: 1px solid black;\">{{ doc.bonus_amount }}</td>\n        </tr>\n    </tbody>\n</table>\n",
